Algoritmo "Salario com 30%"
// Disciplina   : visualg Pacetech
// Autor(a)    : Leandro Paulino Costa
// Data atual  : 10/10/2021
Var
   salario: real
   aumento: real

Inicio
   Escreval ("Informe o salário ")
            Leia (salario)

   aumento <- (salario * 1.3 )
    limpatela
   Escreval ("O salário com aumento é R$:",aumento)

Fimalgoritmo
